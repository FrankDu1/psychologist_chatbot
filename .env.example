# 应用配置 / Application Configuration
APP_NAME=Offerupup
APP_NAME_EN=Offerupup

# 默认API Key每日免费请求次数限制 / Daily free request limit for default API key
DAILY_FREE_LIMIT=10

# 认证配置 - 设置为true强制要求登录 / Authentication - set to true to require login
REQUIRE_AUTH=false

# JWT配置 / JWT Configuration
JWT_SECRET_KEY=your-secret-key-change-this-in-production-use-random-string

# 微信OAuth配置 / WeChat OAuth Configuration
# 前往 https://open.weixin.qq.com/ 注册开放平台账号，创建网站应用后获取以下信息
# Register at https://open.weixin.qq.com/, create website app to get credentials
WECHAT_APP_ID=
WECHAT_APP_SECRET=

# 阿里云 DashScope API Key
DASHSCOPE_API_KEY=your-dashscope-api-key-here

# OpenAI API Key
OPENAI_API_KEY=your-openai-api-key-here


# 聊天API默认配置
DEFAULT_CHAT_ENDPOINT=https://dashscope.aliyuncs.com/compatible-mode/v1/chat/completions
DEFAULT_CHAT_MODEL=qwen-plus
DEFAULT_CHAT_API_KEY=your-chat-api-key-here

# 图片生成默认配置  
DEFAULT_IMAGE_ENDPOINT=https://dashscope.aliyuncs.com/api/v1/services/aigc/multimodal-generation/generation
DEFAULT_IMAGE_MODEL=qwen-image-plus
DEFAULT_IMAGE_SIZE=1328*1328
DEFAULT_IMAGE_API_KEY=your-image-api-key-here

# Agent配置（可选）/ Agent Configuration (Optional)
DEFAULT_AGENT_API_KEY=
AGENT_APP_ID=

# 测试短信验证码（生产环境需接入真实短信服务）/ Test SMS code (Use real SMS service in production)
TEST_SMS_CODE=123456